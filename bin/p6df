#!/usr/bin/env zsh

# shellcheck shell=bash

######################################################################
#<
#
# Function: p6df::main()
#
#  Environment:	 P6_DFZ_LIB_DIR
#>
######################################################################
p6df::main() {

  . $P6_DFZ_LIB_DIR/_bootstrap.zsh
  p6df::core::_bootstrap

  p6df::core::cli::run "$@"
}

p6df::main "$@"
