#!/usr/bin/env zsh

# shellcheck shell=bash

p6df::main() {

  # XXX: hack
  . $P6_DFZ_LIB_DIR/cli.zsh
  . $P6_DFZ_LIB_DIR/file.zsh
  . $P6_DFZ_LIB_DIR/internal.zsh
  . $P6_DFZ_LIB_DIR/module.zsh
  . $P6_DFZ_LIB_DIR/modules.zsh
  . $P6_DFZ_LIB_DIR/prompt.zsh

  # XXX: hack
